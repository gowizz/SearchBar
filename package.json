{
  "name": "@gowiz/searchbar",
  "version": "0.0.1",
  "description": "Searchbars powered by Gowiz search engine technology",
  "main": "lib/gowizSearchbar.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "webpack --config webpack/webpack.config.js --mode production",
    "dev": "webpack --progress --colors --watch --config webpack/webpack.config.js --mode development",
    "test": "jest",
    "format": "prettier --write \"**/*.+(json|md|ts|tsx)\"",
    "lint:css": "npx stylelint \"**/*.css\" --fix"
  },
  "repository": "https://github.com/gowizz/SearchBar",
  "readme": "https://github.com/gowizz/SearchBar/blob/master/README.md",
  "license": "UNLICENSED",
  "author": "Karl-Gustav Kallasmaa <karl.gustav1789@gmail.com> (https://github.com/KGKallasmaa)",
  "keywords": [
    "gowiz",
    "search",
    "engine",
    "searchbar",
    "onsite"
  ],
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__test__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?|ts?)$"
  },
  "pre-commit": [
    "lint:css",
    "format"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.10.4",
    "@types/jest": "^26.0.5",
    "@types/react": "^16.9.43",
    "awesome-typescript-loader": "^5.2.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "chai": "^4.2.0",
    "css-loader": "^3.6.0",
    "jest": "^26.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "source-map-loader": "^1.0.1",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.7",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.0.9"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": ">=16.0.0"
  }
}
