{
  "name": "@gowiz/searchbar",
  "version": "0.2.0",
  "description": "Different search bars powered by Gowiz search engine technology",
  "main": "lib/gowizSearchbar.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "webpack --config webpack/webpack.config.js --mode production",
    "dev": "webpack --progress --colors --watch --config webpack/webpack.config.js --mode development",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint:css": "npx stylelint \"**/*.css\" --fix",
    "format": "prettier --write \"**/*.+(json|md|ts|tsx)\"",
    "prepare": "npm dedupe",
    "type_build": "tsc --declaration"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gowizz/SearchBar.git"
  },
  "readme": "https://github.com/gowizz/SearchBar/blob/master/README.md",
  "license": "UNLICENSED",
  "author": "Karl-Gustav Kallasmaa <karl.gustav1789@gmail.com> (https://github.com/KGKallasmaa)",
  "keywords": [
    "gowiz",
    "search",
    "engine",
    "searchbar",
    "onsite"
  ],
  "bugs": {
    "url": "https://github.com/gowizz/SearchBar/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib"
  ],
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__test__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?|ts?)$"
  },
  "pre-commit": [
    "lint:css",
    "format",
    "type_build"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^11.0.2",
    "@types/jest": "^26.0.8",
    "@types/react": "^16.9.44",
    "@types/react-addons-shallow-compare": "^0.14.22",
    "awesome-typescript-loader": "^5.2.1",
    "axios": "^0.20.0",
    "css-loader": "^4.2.0",
    "fuse.js": "^6.4.1",
    "jest": "^26.2.2",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-addons-shallow-compare": "^15.6.2",
    "react-device-detect": "^1.13.1",
    "react-dom": "^16.13.1",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7",
    "uuidv4": "^6.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.0.9"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  }
}
